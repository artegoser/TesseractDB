{
  "name": "tessradb",
  "version": "0.1.0",
  "description": "A local noSQL database",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "mocha ./tests/tests.spec.js",
    "ctest": "npm run comp && npm run test",
    "coverage": "nyc npm run test",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "co-publish": "npm run comp && npm run coveralls && npm publish",
    "comp": "tsc"
  },
  "nyc": {
    "exclude": "**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artegoser/TessraDB.git"
  },
  "keywords": [
    "noSQL",
    "DB",
    "database",
    "local",
    "localdb",
    "localdatabase"
  ],
  "author": "artegoser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artegoser/TessraDB/issues"
  },
  "homepage": "https://github.com/artegoser/TessraDB#readme",
  "devDependencies": {
    "coveralls": "^3.1.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/node": "^17.0.6",
    "JSONStream": "^1.3.5"
  }
}
